<div id="searches-my-searches" class="tab-pane">
    <h2>My Searches</h2>
    <table id="my-searches">
        <thead>
            <tr>
                <th>Date</th>
                <th>Search String</th>
                <th>&nbsp;</th>
            </tr>
        </thead>
    </table>
</div>
<div id="searches-advance-search" class="tab-pane">
    <h2>Advance Search</h2>
    <form class="form-horizontal" role="form" onsubmit="event.preventDefault(); advanceSearchMovie();">
        <div class="form-group">
            <label for="movie-name" class="col-sm-2 control-label">Name</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="movie-name" placeholder="Name" required>
            </div>
        </div>
        <div class="form-group">
            <label for="movie-year" class="col-sm-2 control-label">Year</label>
            <div class="col-sm-2">
                <input type="number" class="form-control" id="movie-year" min="1900" required>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-default">Search</button>
            </div>
        </div>
    </form>
</div>
<div id="searches-all-searches" class="tab-pane">
    <h2>All Searches</h2>
    <table id="all-searches">
        <thead>
            <tr>
                <th>Date</th>
                <th>Search String</th>
                <th>&nbsp;</th>
            </tr>
        </thead>
        <tbody>

        </tbody>
    </table>
</div>
<div id="searches-graph" class="tab-pane">
    <h2>Common Searches</h2>
    <div>
    </div>
</div>
<div id="searches-filter-searches" class="tab-pane">
    <h2>Filter Searches</h2>
    <form class="form-horizontal" role="form" onsubmit="return false">
        <div class="form-group">
            <label for="filter-search-string" class="col-sm-2 control-label">Search String</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="filter-search-string" placeholder="Search String">
            </div>
        </div>
        <div class="form-group">
            <label for="filter-from" class="col-sm-2 control-label">From</label>
            <div class="col-sm-2">
                <input type="date" class="form-control" id="filter-from">
            </div>
        </div>
        <div class="form-group">
            <label for="filter-to" class="col-sm-2 control-label">To</label>
            <div class="col-sm-2">
                <input type="date" class="form-control" id="filter-to">
            </div>
        </div>
        <div class="form-group">
            <label for="filter-country" class="col-sm-2 control-label">Country</label>
            <div class="col-sm-2">
                <input type="text" class="form-control" id="filter-country" placeholder="Country">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-default" onclick="filterSearches()">Filter</button>
            </div>
        </div>
    </form>
    <div id="searches-filter-results">
        <table id="searches-filter-results-table">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Search String</th>
                    <th>&nbsp;</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


